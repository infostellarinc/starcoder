<?xml version="1.0"?>
<block>
  <name>gRPC Message Sink</name>
  <key>grpcall_msg_sink</key>
  <category>[grpcall]</category>
  <import>import grpcall</import>
  <make>grpcall.msg_sink($address)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>Address</name>
    <key>address</key>
    <value>'localhost:50051'</value>
    <type>string</type>
  </param>

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>message</type>
  </sink>
</block>
