<?xml version="1.0"?>
<block>
  <name>additive_scrambler</name>
  <key>starcoder_utils_additive_scrambler</key>
  <category>[starcoder_utils]</category>
  <import>import starcoder_utils</import>
  <make>starcoder_utils.additive_scrambler($mask, $seed, $length, $count, $bits_per_byte, $reset_tag_key, $enabled)</make>

  <param>
    <name>Mask</name>
    <key>mask</key>
    <value>0x8A</value>
    <type>hex</type>
  </param>
  <param>
    <name>Seed</name>
    <key>seed</key>
    <value>0x7F</value>
    <type>hex</type>
  </param>
  <param>
    <name>Length</name>
    <key>length</key>
    <value>7</value>
    <type>int</type>
  </param>
  <param>
    <name>Count</name>
    <key>count</key>
    <value>0</value>
    <type>int</type>
  </param>
  <param>
    <name>Bits per byte</name>
    <key>bits_per_byte</key>
    <value>1</value>
    <type>int</type>
  </param>
  <param>
    <name>Reset tag key</name>
    <key>reset_tag_key</key>
    <value>""</value>
    <type>string</type>
  </param>

  <param>
    <name>Enabled</name>
    <key>enabled</key>
    <type>bool</type>
    <option>
      <name>No</name>
      <key>False</key>
    </option>
    <option>
      <name>Yes</name>
      <key>True</key>
    </option>
  </param>

  <sink>
    <name>in</name>
    <type>message</type>
  </sink>

  <source>
    <name>out</name>
    <type>message</type>
  </source>
</block>
