<?xml version="1.0"?>
<block>
  <name>pmt_pass</name>
  <key>starcoder_utils_pmt_pass</key>
  <category>[starcoder_utils]</category>
  <import>import starcoder_utils</import>
  <make>starcoder_utils.pmt_pass()</make>

  <sink>
    <name>in</name>
    <type>message</type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>message</type>
  </source>
</block>
