<?xml version="1.0"?>
<block>
  <name>viterbi_decoder</name>
  <key>starcoder_utils_viterbi_decoder</key>
  <category>[starcoder_utils]</category>
  <import>import starcoder_utils</import>
  <make>starcoder_utils.viterbi_decoder($cc_decoder_definition, $packed, $rev_packing, $mtu_bytes, $enabled)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>Decoder Obj.</name>
    <key>cc_decoder_definition</key>
  </param>

  <param>
    <name>Packed</name>
    <key>packed</key>
    <type>bool</type>
    <option>
      <name>No</name>
      <key>False</key>
    </option>
    <option>
      <name>Yes</name>
      <key>True</key>
    </option>
  </param>

  <param>
    <name>Rev. Packing</name>
    <key>rev_packing</key>
    <type>bool</type>
    <option>
      <name>No</name>
      <key>False</key>
    </option>
    <option>
      <name>Yes</name>
      <key>True</key>
    </option>
  </param>

  <param>
    <name>MTU (bytes)</name>
    <key>mtu_bytes</key>
    <type>int</type>
  </param>

  <param>
    <name>Enabled</name>
    <key>enabled</key>
    <type>bool</type>
    <option>
      <name>No</name>
      <key>False</key>
    </option>
    <option>
      <name>Yes</name>
      <key>True</key>
    </option>
  </param>

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>message</type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>message</type>
  </source>
</block>
